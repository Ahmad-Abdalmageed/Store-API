{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "nodemon src/server.ts",
    "watch": "tsc-watch --onSuccess \"node ./dist/server.js\"",
    "build": "npx tsc",
    "dist": "node build/server.js",
    "jasmine": "jasmine",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --config .prettierrc '**/*.ts' --write",
    "up": "db-migrate up",
    "down": "db-migrate down",
    "testDB": "ENV=test db-migrate --env test up && npm run jasmine && db-migrate db:drop test"
  },
  "author": "Ahmad Abdalmageed",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "db-migrate": "^0.11.13",
