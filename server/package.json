{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "A Typescript API used in building a Modern Storefront allowing numerous users to purchase Company's Products",
  "scripts": {
    "clean": "rimraf dist/",
    "zip": "bestzip dist/Archive.zip .",
    "start": "node dist/server.js",
    "build": "npm run clean && npx tsc && cd dist/ && npm run zip && cd ..",
    "deploy": "chmod +x bin/deploy.sh && ./bin/deploy.sh",
    "watch": "tsc-watch --onSuccess \"npm run start\"",
    "test": "db-migrate --env test up && ENV=test jasmine-ts && db-migrate --env test down",
    "up": "db-migrate up",
    "down": "db-migrate down",
    "up-test": "db-migrate --env test up",
    "down-test": "db-migrate --env test down",
    "lint": "eslint .. --ext .ts",
    "prettier": "prettier --config .prettierrc '**/*.ts' --write"
  },
  "author": "Ahmad Abdalmageed",
  "license": "ISC",
  "engines": {
    "node": ">=16.15.1"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "typescript": "^4.7.3"
  },
  "devDependencies": {
    "@angular/cli": "^14.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/pg": "^7.14.7",
    "@types/prettier": "^2.6.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-node": "^10.8.1",
    "tsc-watch": "^4.2.9",
    "bestzip": "^2.2.1",
    "rimraf": "^3.0.2"
  }
}
